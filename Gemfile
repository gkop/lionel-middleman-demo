source :rubygems

gem "middleman", "~>3.0.7"

# bundle lionel from git in order to take advantage of bundler local overrides
gem "lionel", :github => "gkop/lionel", :branch => "master"

# need to bundle from git because the dependencies of the published gem conflict# with those of middleman 3.0.7
gem "jquery-rails", :github => "rails/jquery-rails"

gem "jquery-ui-rails"
gem "underscore-rails"
gem "knockoutjs-rails"
gem "jquery-fileupload-rails"
